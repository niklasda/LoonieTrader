<Window x:Class="LoonieTrader.App.Views.ChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:parts="clr-namespace:LoonieTrader.App.Views.Parts"
        DataContext="{Binding Source={StaticResource Locator}, Path=Chart}"
        Title="Chart"
        ResizeMode="CanResizeWithGrip"
        Height="419"
        Width="473" Icon="/LoonieTrader.App;component/Images/AppBitmap.bmp">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <parts:SciChartPartView Grid.Row="1" DataContext="{Binding .}" />

    </Grid>
</Window>















